[application]

config/name="Cardgame"
run/main_scene="res://scenes/menus/LoadMenu.tscn"
config/icon="res://icon.png"

[autoload]

GlobalSettings="*res://singletons/GlobalSetting.gd"
GlobalCard="*res://singletons/GlobalCard.gd"
GlobalOverlay="*res://scenes/overlay/GlobalOverlay.tscn"
Gateway="*res://singletons/Gateway.gd"
ServerData="*res://singletons/ServerData.gd"
Server="*res://singletons/Server.gd"
Toolbox="*res://singletons/Toolbox.gd"
Cache="*res://singletons/Cache.gd"
Discord="*res://addons/discord_game_sdk/discord.gd"

[debug]

settings/fps/force_fps=60
multirun/number_of_windows=2
multirun/window_distance=1270
multirun/add_custom_args=true
multirun/first_window_args="listen"
multirun/other_window_args="join"

[display]

window/size/width=2560
window/size/height=1440
window/stretch/mode="viewport"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/discord_game_sdk/plugin.cfg", "res://addons/multirun/plugin.cfg" )

[global]

display=false
2d=false
resize=false
resiz=false
stretch=false

